#include <bits/stdc++.h>
using namespace std;


int main() {
	int n;
	cin>>n;
	
	
	int a[n];
	const int N=1e6+2;
	bool b[N];
	
	for(int i=0;i<N;i++)
	{
	    b[i]=false;
	}
	
	for(int i=0;i<n;i++)
	{
	    cin>>a[i];
	    
	    if(a[i]>=0)
	    {
	       b[a[i]]=true; 
	        
	    }
	}
	
	for(int i=1;i<N;i++)
	{
	    if(b[i])
	    continue;
	    
	    else
	    cout<<i<<endl;
	    break;
	    
	    
	 }
	
	
	
	
	return 0;
}
