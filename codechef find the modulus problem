#include<bits/stdc++.h> 
using namespace std;
#define ll long long
int main (){ 
	int t; 
	cin>>t; 
	while(t--){ 
		ll temp= 2000000000000000000;
		printf("? 2000000000000000000\n"); 
		ll n,m; 
		cin>>n; 
		temp-=n; 
		temp--;
		printf("? %lld\n",temp); 
		cin>>m; 
		cout<<"! "<<m+1<<endl;
	}
	return 0;
}
