#include<bits/stdc++.h>
using namespace std;

int findtillxor(int c)
{
     if(c%4==0)
   return c;
    
    else if(c%4==2)
    return ++c;
    
    
    else if(c%4==3)
   return 0;
   
    else if(c%4==1)
    return 1;
    
}
int main()
{
    int a,b;
    cin>> a >> b;
    int x=findtillxor(a);
    int y=findtillxor(b);
    int s=x xor y;
    cout<<s<<endl;
    return 0;
    
    
    
    int c=b-a;
   
    
}
