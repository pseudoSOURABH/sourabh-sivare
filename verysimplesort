#include <iostream>
using namespace std;
#include<bits/stdc++.h>


int main() {
	// your code goes here
	int n;
	cin>>n;
	
	int arr[n];
	
	for(int i=0;i<n;i++)
	{
	      cin>>arr[i];
	}
	
	sort(arr,arr+n);
	for(int i=0;i<n;i++)
	{
	      cout<<arr[i]<<endl;
	}
	return 0;
}

// Output:

// 1
// 3
// 5
// 6
// 7
